{
    "env": {
        "browser": true,
        "es2021": true
    },
    "rules": {
        "semi": [
            1, "always"
        ]
    },
    "extends": [
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ]
}
